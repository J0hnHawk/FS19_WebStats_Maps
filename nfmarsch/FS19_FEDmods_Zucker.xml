            <inputProduct name="IP_PALLET" percent="5"/>
          </inputs>
          <outputs>
            <outputProduct name="OP_ZUCKER" percent="40"/>
            <outputProduct name="OP_COMPOST" percent="60"/>
          </outputs>
        </productLine>
      </productLines>
