        <inputProduct name="IP_LIQUID" capacity="1000000" fillTypes="WATER" fixedPricePerLitre="1.6"/>
        <inputProduct name="IP_PALLET" capacity="100000" fillTypes="LEERPALLET" fixedPricePerLitre="1.6"/>
      </inputProducts>
      <outputProducts>
        <outputProduct name="OP_ZUCKER" fillType="ZUCKER" capacity="1000000"/>
        <outputProduct name="OP_COMPOST" fillType="COMPOST" capacity="500000"/>
      </outputProducts>
      <productLines>
        <productLine outputPerHour="7000">
          <inputs>
            <inputProduct name="IP_SUGAR" percent="90"/>
