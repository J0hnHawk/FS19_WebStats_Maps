<?xml version="1.0" encoding="UTF-8"?>
<mapconfig>
  <objects>
    <object name="FS19_Mischfutterherstellung" locationType="GlobalCompany" showInProduction="true" canPurchaseInputs="true">
      <inputProducts>
        <inputProduct name="IP_DRYGRASS" capacity="500000" fillTypes="DRYGRASS_WINDROW" useSellPointPrice="DRYGRASS_WINDROW" purchaseMultiplier="1.0"/>
        <inputProduct name="IP_STRAW" capacity="500000" fillTypes="STRAW" useSellPointPrice="STRAW" purchaseMultiplier="1.0"/>
        <inputProduct name="IP_SILAGE" capacity="500000" fillTypes="SILAGE" useSellPointPrice="SILAGE" purchaseMultiplier="6.5"/>
      </inputProducts>
      <outputProducts>
        <outputProduct name="OP_FORAGE" fillType="FORAGE" capacity="500000"/>
      </outputProducts>
      <productLines>
        <productLine outputPerHour="10000">
          <inputs>
            <inputProduct name="IP_DRYGRASS" percent="50"/>
            <inputProduct name="IP_STRAW" percent="25"/>
            <inputProduct name="IP_SILAGE" percent="25"/>
          </inputs>
          <outputs>
            <outputProduct name="OP_FORAGE" percent="100"/>
          </outputs>
        </productLine>
      </productLines>
    </object>
